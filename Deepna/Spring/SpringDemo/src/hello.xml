<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="bean1" class="com.sj.beans.HelloBean">
		<property name="msg" value="Hello world from Spring"></property>
	</bean>
<bean id="bean2" class="com.sj.beans.HelloConstr">
		<constructor-arg value="Hello world from Spring Team"></constructor-arg>
	</bean>
</beans>
